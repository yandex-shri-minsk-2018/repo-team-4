{
  "name": "shri-boilerplate",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001/",
  "dependencies": {
    "concurrently": "^3.5.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "mongod": "^2.0.0",
    "mongodb": "^3.0.3",
    "nodemon": "^1.15.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "socket.io-cookie-parser": "^1.0.0",
    "stylus": "^0.54.5",
    "uuid": "^3.2.1"
